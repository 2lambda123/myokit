[run]
source = myokit
omit =
    myokit/gui/*
    myokit/_sim/opencl.py
    myokit/_sim/openclsim.py
    myokit/_sim/fiber_tissue.py
    myokit/_command.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    except ImportError:
    if False:
    if __name__ == .__main__.:
