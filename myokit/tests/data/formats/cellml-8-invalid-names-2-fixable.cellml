<?xml version='1.0' encoding='utf-8'?>
<model name="test"
        xmlns="http://www.cellml.org/cellml/1.0#"
        xmlns:cellml="http://www.cellml.org/cellml/1.0#">
  <component name="Main">
    <variable units="dimensionless" name="_xi" initial_value="-1" />
    <variable units="dimensionless" name="time" />
    <math xmlns="http://www.w3.org/1998/Math/MathML">
      <apply>
        <eq/>
        <apply>
          <diff/>
          <bvar>
            <ci>time</ci>
          </bvar>
          <ci>_xi</ci>
        </apply>
        <apply>
          <cn cellml:units="dimensionless">1</cn>
        </apply>
      </apply>
    </math>
  </component>
</model>
