[[model]]
name: LotkaVolterra
X.x = 10
Y.y = 5

# Simulation engine variables
[engine]
time = 0 bind time

[X]
a = 3
b = b1 + b2
    b1 = 1
    b2 = a - b1 - 1
dot(x) =  a * x - b * x * Y.y

[Y]
c = X.a - 1
d = 2
dot(y) = -c * y + d * X.x * y

[Z]
use X.x, Y.y
total = x + y

[[protocol]]
# Level Start Length Period Multiplier
1.0     100   0.5    1000   0

[[script]]
import myokit
from pylab import *

# Run simulation
m = get_model()
p = get_protocol()
s = myokit.Simulation(m, p)
d = s.run(1000)
